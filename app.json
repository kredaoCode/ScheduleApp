      "runtimeVersion": "1.0.0"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "27796f19-61ac-48eb-96b1-9c8739bb23c9"
      }
    },
    "plugins": [
      [
        "expo-updates",
        {
          "username": "kredao"
        }
      ]
    ],
    "updates": {
      "url": "https://u.expo.dev/27796f19-61ac-48eb-96b1-9c8739bb23c9"
